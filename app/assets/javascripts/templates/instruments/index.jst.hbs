<div class="btn-toolbar">
  <div class="btn-group pull-right">
    <button href="#" class="btn btn-success create-metric">Create Instrument</button>
  </div>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Modified</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    {{#each instruments}}
      <tr>
        <td><a href="#" data-navigation-url="/instruments/{{id}}">{{name}}</a></td>
        <td>{{dateFormat updated_at}}</td>
        <td>{{dateFormat created_at}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
